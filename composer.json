    "require": {
        "php": ">=7.2.0",
        "topthink/framework": "6.0.*",
        "ramsey/uuid": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "think\\Laradumps\\": "src"
        },
        "files": [
            "src/functions.php"
